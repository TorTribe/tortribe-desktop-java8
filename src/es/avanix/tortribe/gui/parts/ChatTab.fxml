<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Tab?>

<Tab xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.avanix.tortribe.gui.parts.ChatTabController">
    <content>
        <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0">
            <children>
                <TextArea fx:id="main_txtChat" editable="false" layoutX="42.0" layoutY="9.0" prefHeight="474.0" prefWidth="600.0" wrapText="true" AnchorPane.bottomAnchor="52.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                <TextField fx:id="main_chatMessage" editable="false" layoutX="7.0" layoutY="475.0" prefHeight="50.0" prefWidth="366.0" promptText="Message to send" AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="82.0" />
                <Button fx:id="main_chatBtnSend" defaultButton="true" disable="true" layoutX="518.0" layoutY="474.0" mnemonicParsing="false" onAction="#handleSendMessageButtonAction" prefHeight="50.0" prefWidth="82.0" text="SEND" AnchorPane.bottomAnchor="1.0" AnchorPane.rightAnchor="1.0" />
            </children>
        </AnchorPane>  
    </content>
</Tab>
